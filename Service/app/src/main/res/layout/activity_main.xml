<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/openUpdateBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开更新服务"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.146" />

    <Button
        android:id="@+id/closeUpdateBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭更新服务"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openUpdateBtn"
        app:layout_constraintVertical_bias="0.112" />

    <Button
        android:id="@+id/openMusicBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开音乐服务"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/closeUpdateBtn"
        app:layout_constraintVertical_bias="0.105" />

    <Button
        android:id="@+id/closeMusicBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭音乐服务"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openMusicBtn"
        app:layout_constraintVertical_bias="0.155" />

    <Button
        android:id="@+id/IntentServiceBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开IntentService"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/closeMusicBtn"
        app:layout_constraintVertical_bias="0.178" />
</androidx.constraintlayout.widget.ConstraintLayout>